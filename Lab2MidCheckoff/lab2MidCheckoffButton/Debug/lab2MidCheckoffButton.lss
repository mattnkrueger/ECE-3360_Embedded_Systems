000036 c016                        rjmp IncDisp;
                                 disp5:
000037 e60d                        ldi R16, 0x6d; 5
000038 c014                        rjmp IncDisp;
                                 disp6:
000039 e70d                        ldi R16, 0x7d; 6
00003a c012                        rjmp IncDisp;
                                 disp7:
00003b e007                        ldi R16, 0x07; 7
00003c c010                        rjmp IncDisp;
                                 disp8:
00003d e70f                        ldi R16, 0x7f; 8
00003e c00e                        rjmp IncDisp;
                                 disp9:
00003f e60f                        ldi R16, 0x6f; 9
000040 c00c                        rjmp IncDisp;
                                 dispA:	
000041 e707                        ldi R16, 0x77; A
000042 c00a                        rjmp IncDisp;
                                 dispb:
000043 e70c                        ldi R16, 0x7c; b
000044 c008                        rjmp IncDisp;
                                 dispC:
000045 e309                        ldi R16, 0x39; C
000046 c006                        rjmp IncDisp;
                                 dispd:
000047 e50e                        ldi R16, 0x5e; d
000048 c004                        rjmp IncDisp;
                                 dispE:
000049 e709                        ldi R16, 0x79; E
00004a c002                        rjmp IncDisp;
                                 dispf:
00004b e701                        ldi R16, 0x71; f
00004c c000                        rjmp IncDisp;
                                 
                                 IncDisp:
00004d d079                        rcall display;
00004e cfba                        rjmp IncNumberCheck;
                                 
                                 DecNumberCheck: 
00004f d045                        rcall ButtonCheck;
000050 ff60                        sbrs R22, 0;
000051 cfb7                        rjmp IncNumberCheck;
000052 3050                        cpi R21, 0x00;
000053 f0f9                        breq disp0Dec;
000054 3051                        cpi R21, 0x01;
000055 f0f9                        breq disp1Dec;
000056 3052                        cpi R21, 0x02;
000057 f0f9                        breq disp2Dec;
000058 3053                        cpi R21, 0x03;
000059 f0f9                        breq disp3Dec;
00005a 3054                        cpi R21, 0x04;
00005b f0f9                        breq disp4Dec;
00005c 3055                        cpi R21, 0x05;
00005d f0f9                        breq disp5Dec;
00005e 3056                        cpi R21, 0x06;
00005f f0f9                        breq disp6Dec;
000060 3057                        cpi R21, 0x07;
000061 f0f9                        breq disp7Dec;
000062 3058                        cpi R21, 0x08;
000063 f0f9                        breq disp8Dec;
000064 3059                        cpi R21, 0x09;
000065 f0f9                        breq disp9Dec;
000066 305a                        cpi R21, 0x0a;
000067 f0f9                        breq dispADec;
000068 305b                        cpi R21, 0x0b;
000069 f0f9                        breq dispbDec;
00006a 305c                        cpi R21, 0x0c;
00006b f0f9                        breq dispCDec;
00006c 305d                        cpi R21, 0x0d;
00006d f0f9                        breq dispdDec;
00006e 305e                        cpi R21, 0x0e;
00006f f0f9                        breq dispEDec;
000070 305f                        cpi R21, 0x0f;
000071 f0f9                        breq dispfDec;
000072 cfdc                        rjmp DecNumberCheck;
                                 
                                 disp0Dec:
000073 eb0f                        ldi R16, 0xbf; 0 with decimal
000074 c01e                        rjmp DispDec;
                                 disp1Dec:
000075 e806                        ldi R16, 0x86; 1 with decimal
000076 c01c                        rjmp DispDec;
                                 disp2Dec:
000077 ed0b                        ldi R16, 0xdb; 2 with decimal
000078 c01a                        rjmp DispDec;
                                 disp3Dec:
000079 ec0f                        ldi R16, 0xcf; 3 with decimal
00007a c018                        rjmp DispDec;
                                 disp4Dec:
