000084 c00e                        rjmp DispDec;
                                 disp9Dec:
000085 ee0f                        ldi R16, 0xef; 9 with decimal
000086 c00c                        rjmp DispDec;
                                 dispADec:
000087 ef07                        ldi R16, 0xf7; A with decimal
000088 c00a                        rjmp DispDec;
                                 dispbDec:
000089 ef0c                        ldi R16, 0xfc; b with decimal
00008a c008                        rjmp DispDec;
                                 dispCDec:
00008b eb09                        ldi R16, 0xb9; C with decimal
00008c c006                        rjmp DispDec;
                                 dispdDec:
00008d ed0e                        ldi R16, 0xde; d with decimal
00008e c004                        rjmp DispDec;
                                 dispEDec:
00008f ef09                        ldi R16, 0xf9; E with decimal
000090 c002                        rjmp DispDec;
                                 dispfDec:
000091 ef01                        ldi R16, 0xf1; f with decimal
000092 c000                        rjmp DispDec;
                                 
                                 DispDec:
000093 d033                        rcall display;
000094 cfba                        rjmp DecNumberCheck;
                                 
                                 ButtonCheck:
000095 991b                        sbic PINB, 3; skip if button is pressed (if line is low skip) (a button press makes the line low)
000096 9508                        ret;(button not pressed jump back to display loop)
                                 ButtonPressLoop:
000097 d044                        rcall Delay;
000098 9711                        sbiw R27:R26, 1;
000099 f0b1                        breq OneToTwo;
00009a 9b1b                        sbis PINB, 3; skip if the button has been released (line is back to high)
00009b cffb                        rjmp ButtonPressLoop;
00009c 3061                        cpi R22, 1;
00009d f049                        breq DecButtonCheck;
                                 IncButtonCheck:
00009e 305f                        cpi R21, 0x0f;
00009f f021                        breq rolloverInc;
0000a0 3071                        cpi R23, 1;
0000a1 f101                        breq WaitForRelease;
0000a2 9553                        inc R21;
0000a3 c01e                        rjmp WaitForRelease;
                                 rolloverInc:
0000a4 e050                        ldi R21, 0x00;
0000a5 e071                        ldi R23, 1;
0000a6 c01b                        rjmp WaitForRelease
                                 DecButtonCheck:
0000a7 3050                        cpi R21, 0x00;
0000a8 f021                        breq rolloverDec;
0000a9 3071                        cpi R23, 1;
0000aa f0b9                        breq WaitForRelease;
0000ab 955a                        dec R21;
0000ac c015                        rjmp WaitForRelease;
                                 rolloverDec:
0000ad e05f                        ldi R21, 0x0f;
0000ae e071                        ldi R23, 1;
0000af c012                        rjmp WaitForRelease;
                                 OneToTwo:
                                   ;reset the counter
0000b0 eea8                        ldi R26, 0xe8; resets counter to 1000 (1 second limit)
0000b1 e0b3                        ldi R27, 0x03; resets counter to 1000 (1 second limit)
                                   OneToTwoLoop:
0000b2 d029                          rcall Delay
0000b3 9711                          sbiw R27:R26, 1;
0000b4 f041                          breq TwoToThree;
0000b5 9b1b                          sbis PINB, 3; skip if the button has been released (line is back to high)
0000b6 cffb                          rjmp OneToTwoLoop;
0000b7 3071                      	cpi R23, 1;
0000b8 f049                      	breq WaitForRelease;
0000b9 3061                          cpi R22, 1; if we are in decrement mode
0000ba f031                          breq IncMode
0000bb e061                          ldi R22, 1; switch to decrement mode
0000bc c005                          rjmp WaitForRelease;
                                 TwoToThree:
0000bd e070                        ldi R23, 0;
0000be e060                        ldi R22, 0; reset, we are in increment mode
0000bf e050                        ldi R21, 0; display 0
0000c0 c001                        rjmp WaitForRelease;
                                 IncMode:; switch to increment mode
0000c1 e060                        ldi R22, 0;
                                 WaitForRelease:
0000c2 9b1b                        sbis PINB, 3; skip if the button has been released (line is back to high)
0000c3 cffe                        rjmp WaitForRelease;
0000c4 eea8                        ldi R26, 0xe8; resets counter to 1000 (1 second limit)
0000c5 e0b3                        ldi R27, 0x03; resets counter to 1000 (1 second limit)
0000c6 9508                        ret;
                                 
                                 ; ---- Display
                                 ;
                                 ;   Output hexidecimal bit representation to 7-Segment display utilizing stack and 74hc595 shift register for storage
                                 display:
0000c7 930f                        push R16
0000c8 931f                        push R17
0000c9 b71f                        in R17, SREG
0000ca 931f                        push R17
0000cb e018                        ldi R17, 8
                                   
                                   ; Loop
                                   loop:
0000cc 1f00                          rol R16
0000cd f010                          BRCS set_ser_in_1
0000ce 9828                          cbi PORTB, 0
0000cf c001                        rjmp end
                                 
                                   ; Set SER Input High
                                   set_ser_in_1:
0000d0 9a28                          sbi PORTB, 0
                                 
                                   ; End
                                   end:
0000d1 9a2a                          sbi PORTB, 2
0000d2 982a                          cbi PORTB, 2
0000d3 951a                          dec R17
0000d4 f7b9                          brne loop
0000d5 9a29                          sbi PORTB, 1
0000d6 9829                          cbi PORTB, 1
                                 
0000d7 911f                        pop R17
0000d8 bf1f                        out SREG, R17
0000d9 911f                        pop R17
0000da 910f                        pop R16
0000db 9508                      ret;
                                 
                                 ;This delay loop lasts for about 1ms (check button every ms)
                                 Delay:
0000dc e5ed                      	ldi r30, 0x5d	  	; r31:r30  <-- load a 16-bit value into counter register for outer loop
0000dd e0f0                      	ldi r31, 0x00;
                                 d1:
0000de e2da                      	ldi   r29, 0x2a		    	; r29 <-- load a 8-bit value into counter register for inner loop
                                 d2:
0000df 0000                      	nop				; no operation
0000e0 95da                      	dec   r29            		; r29 <-- r29 - 1
0000e1 f7e9                      	brne  d2			; branch to d2 if result is not "0"
0000e2 9731                      	sbiw r31:r30, 1			; r31:r30 <-- r31:r30 - 1
0000e3 f7d1                      	brne d1				; branch to d1 if result is not "0"


