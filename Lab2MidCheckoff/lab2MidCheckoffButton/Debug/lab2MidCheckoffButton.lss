00007c c016                        rjmp DispDec;
                                 disp5Dec:
00007d ee0d                        ldi R16, 0xed; 5 with decimal
00007e c014                        rjmp DispDec;
                                 disp6Dec:
00007f ef0d                        ldi R16, 0xfd; 6 with decimal
000080 c012                        rjmp DispDec;
                                 disp7Dec:
000081 e807                        ldi R16, 0x87; 7 with decimal
000082 c010                        rjmp DispDec;
                                 disp8Dec:
